{
    "compilerOptions": {
      "target": "es2015",
      "lib": [
        "es2015"
      ],
      "module": "commonjs",
      "rootDir": "./",
      "allowJs": true,
      "isolatedModules": false,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "noImplicitAny": false, // 是否在表达式和申明上有隐含的any类型时报错
    }
  }
